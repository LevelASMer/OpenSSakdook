<div metal:use-macro="layout">
  <div metal:fill-slot="content">
    <h1>View Logs</h1>
    <table>
      <tbody>
        <tr>
          <th>Username</th>
          <th>Description</th>
          <th>Date</th>
        </tr>
        <tr tal:repeat="logs log">
          <td>${logs[1]}</td>
          <td>${logs[2]}</td>
          <td>${logs[3]}</td>
        </tr>
      </tbody>
    </table>
  </div>
</div>